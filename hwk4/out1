T_0 ;
    %wait E_0x1d49760;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x1d4e4d0_0, 0, 1;
    %load/vec4 v0x1d675e0_0;
    %parti/s 1, 4, 4;
    %load/vec4 v0x1d676a0_0;
    %parti/s 1, 4, 4;
    %cmp/e;
    %jmp/0xz  T_0.0, 4;
    %load/vec4 v0x1d675e0_0;
    %parti/s 4, 0, 2;
    %pad/u 5;
    %load/vec4 v0x1d676a0_0;
    %parti/s 4, 0, 2;
    %pad/u 5;
    %add;
    %split/vec4 4;
    %ix/load 4, 0, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x1d67500_0, 4, 4;
    %store/vec4 v0x1d4e4d0_0, 0, 1;
    %load/vec4 v0x1d675e0_0;
    %parti/s 1, 4, 4;
    %ix/load 4, 4, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x1d67500_0, 4, 1;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x1d676a0_0;
    %parti/s 4, 0, 2;
    %load/vec4 v0x1d675e0_0;
    %parti/s 4, 0, 2;
    %cmp/u;
    %flag_or 5, 4;
    %jmp/0xz  T_0.2, 5;
    %load/vec4 v0x1d675e0_0;
    %parti/s 4, 0, 2;
    %load/vec4 v0x1d676a0_0;
    %parti/s 4, 0, 2;
    %sub;
    %ix/load 4, 0, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x1d67500_0, 4, 4;
    %load/vec4 v0x1d675e0_0;
    %parti/s 1, 4, 4;
